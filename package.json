{
  "name": "lucifer-cloud-dashboard",
  "private": true,
  "version": "1.0.1",
  "type": "module",
  "scripts": {
    "debug": "vite --mode dev --debug",
    "dev": "vite --mode dev",
    "prod": "vite --mode prod",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
